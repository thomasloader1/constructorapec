mixin card-bug(bugs)
  each bug in bugs
    .col-sm-6.col-md-4.my-3.d-flex.align-items-streach.justify-content-streach
      a(data-toggle="modal", data-target=`#${bug.modal.id}`).card.text-center.w-100.rounded-xl
        .card-body.d-flex.align-items-start.justify-content-center
          .my-3
            .square-element-6.mx-auto.rounded-circle.bg-dark.d-flex.align-items-center.justify-content-center.p-2
              if(bug.img.split(".")[1] != "svg")
                i(class=`${bug.img} text-white fa-4x`)
              else
                img(src=`./img/svg/${bug.img}`, alt=`${bug.name}`).img-plague
            hr.w-75.mx-auto.my-4.danger-color.pt-1.rounded
            p.lato.text-uppercase.font-18.mb-1.text-dark.spacing-2.my-auto #{bug.name}
    .modal.fade(id=`${bug.modal.id}`, tabindex="-1", role="dialog", aria-labelledby=`${bug.modal.title}`, aria-hidden="true")
      .modal-dialog.modal-dialog-centered(role="document")
        .modal-content.rounded-xl
          .modal-header.d-flex.align-items-center.justify-content-center.flex-column.text-center.position-relative.text-white.bg-dark.rounded-xl-top
            button(type="button", data-dismiss="modal", aria-label="Close").btn-close-modal.red.z-depth-1.position-absolute.bottom-85
              span(aria-hidden="true") &times;
            h5.modal-title.text-uppercase.font-weight-bold.spacing-2.mt-2(id=`modalTitle${bug.modal.id}`)= bug.modal.title
          .modal-body(id=`${bug.modal.id}-content`) #{bug.modal.description}
