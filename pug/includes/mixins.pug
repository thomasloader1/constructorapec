mixin sub-navbar(datas)
    - let arrCopy = datas.slice();
    - let lastElement = arrCopy.pop();
    - let boton = datas[0]
      .container
        .row
          .col-12.d-flex.justify-content-center.my-3.rounded-lg.py-3
            .navbar.navbar-expand-lg.navbar-dark.bg-dark.rounded-xl.p-0.w-100.p-2
              button(type="button" data-toggle="collapse" data-target="#basicExampleSubNav" aria-controls="basicExampleSubNav" aria-expanded="false" aria-label="Toggle navigation").navbar-toggler.d-flex.justify-content-center.align-items-center.font-weight-bold.py-2.w-100.d-lg-none.text-uppercase.spacing-2.font-22#btnBug= boton
                .z-depth-1.arrow-sub-navbar.rounded-circle.red.ml-4.d-flex.align-items-center.justify-content-center
                  i.fas.fa-chevron-down.fa-xs 
              
              .collapse.navbar-collapse.text-uppercase.spacing-2.red.rounded-xl#basicExampleSubNav
                each data,i in datas
                  ul.navbar-nav.text-md-center
                    li.nav-item.ml-2
                      a.nav-link(href="#")= data
