mixin card-factory(cards)
  each card in cards
    .col-sm-12.col-md-4.d-flex.align-items-stretch.justify-content-center.my-2.my-sm-3
      div(class= `img-service-${card.image}`).card.rounded.rounded-lg.w-100
        .card-body.position-relative.z-index-3
          h4.card-title 
            a(href= card.link).text-white #{card.title}
          hr.pt-1.white.w-100
          p.card-text.text-white.mb-5 #{card.description}
          .text-center.position-absolute.bottom-10.left-0.right-0
            a(href= card.link).btn.btn-outline-white.text-uppercase saber mas
